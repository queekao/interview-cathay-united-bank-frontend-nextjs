    "@typescript-eslint/parser": "^6.15.0",
    "cypress": "^13.7.1",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^5.2.0",
